   ],
   "source": [
    "arr = np.array([1, 5, 10, 3, 8, 12, 4])\n",
    "avg = np.average(arr)\n",
    "diffs = [x - avg for x in arr]\n",
    "sqdiffs = np.square(diffs)\n",
    "var = np.sum(sqdiffs)/arr.size\n",
    "std = np.sqrt(var)"
   ]
